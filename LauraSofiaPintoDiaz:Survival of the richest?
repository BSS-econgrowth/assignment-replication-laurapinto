   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "keep hijos_hombres hijas_mujeres estrato edad_unionm sexo edad estado_civil etnia afiliacion edad_thijo nivel_educ rango_ingreso anio familias_accion"
